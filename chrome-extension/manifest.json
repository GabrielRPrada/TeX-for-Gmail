{
  "manifest_version": 2,
  "name": "LaTeX for Gmail",
  "description": "Use LaTeX with Gmail.",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus",
    "https://mail.google.com/",
    "https://blank.org/*"
  ],
  "background": {
    "page": "src/background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://blank.org/*"
      ],
      "document_idle": "document_idle",
      "js": [
        "src/communicator.js",
        "src/portwrapper.js",
        "src/contentscr.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "chrome-extension:/*",
    "blob:*"
  ]
}